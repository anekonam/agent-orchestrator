# FAB Agents Dashboard Environment Configuration

# Azure AI Foundry Configuration (Primary)
# Copy this file to .env and fill in your Azure AI Foundry details

# Azure AI Foundry Endpoint (required)
REACT_APP_AZURE_AI_ENDPOINT=https://your-azure-ai-endpoint.openai.azure.com

# Azure AI API Key (required)
REACT_APP_AZURE_AI_API_KEY=your-azure-ai-api-key-here

# Azure AI API Version (optional, defaults to 2024-02-15-preview)
REACT_APP_AZURE_AI_API_VERSION=2024-02-15-preview

# Azure AI Deployment Name (required)
REACT_APP_AZURE_AI_DEPLOYMENT_NAME=gpt-4

# Azure AI Configuration (optional)
REACT_APP_AZURE_AI_MAX_TOKENS=4000
REACT_APP_AZURE_AI_TEMPERATURE=0.7
REACT_APP_AZURE_AI_TIMEOUT=30000

# Application Configuration
REACT_APP_USE_AZURE_AI=true
REACT_APP_ENABLE_DEBUG=false

# Development Settings
REACT_APP_ENV=development

# Legacy Backend Configuration (disabled by default)
# Only enable if you want to use the old backend instead of Azure AI
REACT_APP_API_BASE_URL=http://api.fab-agents.columbus.tech